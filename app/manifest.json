{
	"manifest_version":2,
	"name":"MuteThisAd",
	"version":"0.001",
	"author":"ThOpak",
	"description":"Mutes the video when an ad is on.",
	"icons":{
    	"16":"",
    	"48":"",
    	"128":""
    	},
    "page_action":{
        "default_icon":{
        	"16":"",
        	"48":"",
        	"128":""
            }
        },
	"content_scripts":[
		{
		"matches":["https://www.youtube.com/watch*"],
		"js":["content.js"]
		}
		],
    "chrome_url_overrides": {
      "newtab": "main.html"
    }
}
